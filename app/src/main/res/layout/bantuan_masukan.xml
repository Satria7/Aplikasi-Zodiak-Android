    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_satu_bantuanDanMasukan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/AppTheme.AppBarOverlay" />
    <View
        android:id="@+id/bts"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/toolbar_satu_bantuanDanMasukan"
        android:background="@color/colorPrimaryDark" />

    <android.support.design.widget.TabLayout
        android:id="@+id/tablayout_tentang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimaryDark"
        android:clickable="true"
        app:tabIndicatorColor="@android:color/white"
        app:tabSelectedTextColor="@android:color/white"
        app:tabTextColor="@color/backgroundTab2"
        android:layout_below="@+id/toolbar_satu_bantuanDanMasukan"
        android:layout_alignParentStart="true">

        <android.support.design.widget.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Bantuan" />

        <android.support.design.widget.TabItem
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Masukan" />

    </android.support.design.widget.TabLayout>

    <View
        android:id="@+id/bts2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/tablayout_tentang"
        android:alpha=".5"
        android:background="@color/colorPrimaryDark" />

    <android.support.v4.view.ViewPager
        android:id="@+id/pager_programstudi"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/tablayout_tentang" />

